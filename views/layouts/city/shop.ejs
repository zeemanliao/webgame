      <div class="item_type">
          <radio class="selectType" data="shopBuy">
              購買
          </radio>
          <radio class="selectType" data="shopSell">
              賣出
          </radio>
          <radio class="selectType" data="shopReSell">
              買回
          </radio>
      </div> 
    <div class="city_content">
      <ul id="shopBuy" class="itemList">
          <li>
          <div class="label" gid="nam">木劍</div>
          <div gid="attr">傷害力 +10</div>
          <div class="number" gid="coins">$1000</div>
          <btn>Buy</btn>
          </li>
      </ul>
      <ul id="shopSell" class="itemList">
      </ul>
      <ul id="shopReSell" class="itemList">
      </ul>
    </div>
</div>