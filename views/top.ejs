<!DOCTYPE html>
<html>

<head>
    <title>
        <%=title%>
    </title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="utf-8">
    <!--
    <script src="http://www.google.com/jsapi"></script>
    <script>
    google.load("jquery", "1");
    google.load("jqueryui", "1");
    </script>
-->
    <script src="/javascripts/jquery-1.11.1.min.js"></script>
    <script src="/javascripts/jquery-ui-1.10.4.custom.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
</head>

<body>
    <div CLASS="msg_frame" id="input_frame">
        <div CLASS="msg_box" id="msg_box">
            <h2 id="title">輸入</h2>
            <hr>
            <input type="text" id="data">
            <div class="btn" gid="ok">確定</div>
            <div class="btn" gid="cl">取消</div>
        </div>
    </div>
    <div CLASS="msg_frame" id="msg_frame">
        <div CLASS="msg_box" id="msg_box">
            <h2>訊息</h2>
            <hr>
            <h3 id="msg"></h3>
        </div>
    </div>
    <div CLASS="msg_frame" id="load_frame">
        <div CLASS="msg_box">
            <h2>初始化中</h2>
            <hr>
            <h3 id="msg">Loading....</h3>
        </div>
    </div>
    <div id="content">
        <div id="show_time">

        </div>
        <div id="chara_frame">
            <div id="main">
                <img alt="Chara Photo" gid="photo">
                <div class="label" gid="nam">Loaing...</div>
                <div class="number" gid="level">Loaing...</div>
                <div class="number" gid="retime">Loaing...</div>
                <div class="number" gid="gold">Loaing...</div>
                <div class="number" gid="vipgold">Loaing...</div>
                <btn class="cricle" gid="add">+</btn>
            </div>
            <ul id="info">
                <li>
                    <div class="label">力量:</div>
                    <div class="number" gid="str">Loading...</div>
                </li>
                <li>
                    <div class="label">智商:</div>
                    <div class="number" gid="int">Loading...</div>
                </li>
                <li>
                    <div class="label">技力:</div>
                    <div class="number" gid="ski">Loading...</div>
                </li>
                <li>
                    <div class="label">敏捷:</div>
                    <div class="number" gid="dex">Loading...</div>
                </li>
                <li>
                    <div class="label">體質:</div>
                    <div class="number" gid="con">Loading...</div>
                </li>
            </ul>

        </div>
    
    <div class="content_frame" id="city_frame">
        <h2 gid="nam">City Name</h2>
        <h3 gid="desc">City Desc</h3>
        <div class="city_content">
            <btn class="city_button" gid="city_quest_btn" data="quest">
                Quest
            </btn>
            <btn class="city_button" gid="city_pet_btn" data="peta">
                Pet
            </btn>
            <btn class="city_button" gid="city_mix_btn" data="mix">
                Mix
            </btn>
            <btn class="city_button" gid="city_act_btn" data="act">
                Act
            </btn>
            <btn class="city_button" gid="city_shop_btn" data="shop">
                Shop
            </btn>
            <btn class="city_button" gid="city_area_btn" data="area">
                Area
            </btn>
            <btn class="city_button" gid="city_cha_btn" data="cha">
                Cha
            </btn>
        </div>
    </div>
    <div id="city">
<%include layouts/city/guest%>
<%include layouts/city/quest%>
<%include layouts/city/act%>
<%include layouts/city/pet%>
<%include layouts/city/mix%>
<%include layouts/city/area%>
<%include layouts/city/map%>
<%include layouts/city/cha%>
<%include layouts/city/shop%>
<%include layouts/city/team%>
    </div>
    <div id="battle">
        <div id="battle_frame">
            <btn class="battle_button" gid="leave_battle_btn" data="city">離開
                <BR>Battle</btn>
            <div id="battle_content">
                <div id="enemy">
                    <enemy pos="1">
                        <span class="nam">M 1</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="2">
                        <span class="nam">M 2</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="3">
                        <span class="nam">M 3</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="4">
                        <span class="nam">M 4</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="5">
                        <span class="nam">M 5</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="6">
                        <span class="nam">M 6</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="7">
                        <span class="nam">M 7</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="8">
                        <span class="nam">M 8</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="9">
                        <span class="nam">M 9</span>
                        <hp></hp>
                    </enemy>
                    <enemy pos="10">
                        <span class="nam">M 10</span>
                        <hp></hp>
                    </enemy>
                </div>
                <div id="team">
                    <member pos="1">
                        <span class="nam">P Name</span>
                        <hp></hp>
                    </member>
                    <member pos="2"></member>
                    <member pos="3"></member>
                    <member pos="4"></member>
                    <member pos="5"></member>
                </div>
            </div>
            <div id="skill_frame">
                <sel class="skill_button" gid="skill1" data="1">Skill 1</sel>
                <sel class="skill_button" gid="skill2" data="2">Skill 2</sel>
                <sel class="skill_button" gid="skill3" data="3">Skill 3</sel>
                <sel class="skill_button" gid="skill4" data="4">Skill 4</sel>
                <sel class="skill_button" gid="skill5" data="5$">Skill 5</sel>
            </div>
            <div id="cmd_frame">
                <btn class="cmd_button" gid="cmd1" data="1">Cmd 1</btn>
                <btn class="cmd_button" gid="cmd2" data="2">Cmd 2</btn>
                <btn class="cmd_button" gid="cmd3" data="3">Cmd 3</btn>
                <btn class="cmd_button" gid="cmd4" data="4">Cmd 4</btn>
                <btn class="cmd_button" gid="cmd5" data="5">Cmd 5</btn>
            </div>
        </div>
        <div id="battle_log">
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
            <p>
                <span></span>.............Test</p>
        </div>
    </div>
    </div>
    </div>
    </div>
    <script>
    window.onload = function() { 
      <%include js/Modernizr.cjs%> 
      <%include js/main.cjs%> 
      <%include js/baseclass.cjs%> 
      <%include js/Route.cjs%> 
      <%-classjs%> 
      <%include js/obj.cjs%> 
      <%include js/event.cjs%>


        window.onresize = function(event) {

        }
    }
    </script>
</body>

</html>
